type TapbuyRedirectResult {
    redirect: Boolean!
    redirect_url: String
    message: String
    pixel_url: String
}

input TapbuyRedirectInput {
    cart_id: String!
    force_redirect: String
    cookies: String
}

extend type Query {
    tapbuyRedirect(input: TapbuyRedirectInput!): TapbuyRedirectResult @resolver(class: "Tapbuy\\RedirectTracking\\Model\\Resolver\\Redirect")
}
